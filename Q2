class Main {
  public static void main(String[] args) {
    int[][] tda = new int[][] {{1,2,3},{4,5,6},{7,8,9},{1,1,1}};
      tda[3][2] = 9;
      tda[1][2] = 0;
      for (int i = 0; i < tda.length; i++) {
          System.out.println();
        for (int j = 0; j < tda[i].length; j++) {
            System.out.print(tda[i] [j] + " ");
        }
      }
  }
}
